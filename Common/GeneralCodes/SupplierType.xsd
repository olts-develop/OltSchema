<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:complexType name="SupplierType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="Seq" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>Supplier Sequence</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Nr" type="xs:integer">
                    <xs:annotation>
                        <xs:documentation>Internal TO Online customer number for this supplier</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="AirlineNr" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>If a supplier is an airline, this node would contain the airline number</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Service" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>Bsp: 060 oder 010</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Code" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>Bsp: IMHO oder KUON</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ApiCode" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="AL" />
                            <xs:enumeration value="ZL" />
                            <xs:enumeration value="FTI" />
                            <xs:enumeration value="ZE" />
                            <xs:enumeration value="CAIN" />
                            <xs:enumeration value="AOT" />
                            <xs:enumeration value="JV" />
                            <xs:enumeration value="ATP" />
                            <xs:enumeration value="TIO" />
                            <xs:enumeration value="BON" />
                            <xs:enumeration value="TUICTSPEG" />
                            <xs:enumeration value="SIHA" />
                            <xs:enumeration value="BLUMAR" />
                            <xs:enumeration value="PRONTO" />
                            <xs:enumeration value="pronto" />
                            <xs:enumeration value="TUICTS" />
                            <xs:enumeration value="tuicts" />
                            <xs:enumeration value="TICO" />
                            <xs:enumeration value="COMRES" />
                            <xs:enumeration value="comres" />
                            <xs:enumeration value="TOURICO" />
                            <xs:enumeration value="tourico" />
                            <xs:enumeration value="GTA" />
                            <xs:enumeration value="HOTELBEDS" />
                            <xs:enumeration value="IVECTOR" />
                            <xs:enumeration value="TRAVELFUSN" />
                            <xs:enumeration value="SUN" />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="Broker" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="PEG" />
                            <xs:enumeration value="Hotelbeds" />
                            <xs:enumeration value="IRIS" />
                            <xs:enumeration value="HOTELBEDS" />
                            <xs:enumeration value="TOURICO" />
                            <xs:enumeration value="COMRES" />
                            <xs:enumeration value="comres" />
                            <xs:enumeration value="PRONTO" />
                            <xs:enumeration value="pronto" />
                            <xs:enumeration value="GTA" />
                            <xs:enumeration value="SUNHOTELS" />
                            <xs:enumeration value="EXCLUSIVELY HOTELS" />
                            <xs:enumeration value="JACOB ONLINE" />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="PayableTo" type="xs:string" use="optional" />
                <xs:attribute name="City" type="xs:string" />
                <xs:attribute name="Name" type="xs:string" />
                <xs:attribute name="Email" type="xs:string" />
                <xs:attribute name="Address1" type="xs:string" />
                <xs:attribute name="Address2" type="xs:string" />
                <xs:attribute name="PostalCode" type="xs:string" />
                <xs:attribute name="SupplierNr" type="xs:string" />
                <xs:attribute name="CountryIsoCode" type="xs:string" />
                <xs:attribute name="BusinessFaxNumber" type="xs:string" />
                <xs:attribute name="CountryPhonePrefix" type="xs:string" />
                <xs:attribute name="BusinessFaxAreaCode" type="xs:string" />
                <xs:attribute name="BusinessPhoneNumber" type="xs:string" />
                <xs:attribute name="BusinessPhoneAreaCode" type="xs:string" />
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
</xs:schema>